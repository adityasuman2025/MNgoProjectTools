{
    "name": "mngo-project-tools",
    "version": "1.3.31",
    "private": false,
    "sideEffects": false,
    "main": "index.ts",
    "module": "index.ts",
    "homepage": "./",
    "files": [
        "*"
    ],
    "repository": {
        "type": "git",
        "url": "git@github.com:adityasuman2025/MNgoProjectTools.git"
    },
    "dependencies": {
        "crypto-js": "^4.0.0",
        "dayjs": "^1.10.4"
    },
    "devDependencies": {
        "@types/node": "^16.18.61",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "typescript": "^4.9.5",
        "@babel/cli": "^7.23.0",
        "@babel/preset-env": "^7.23.3",
        "@babel/preset-react": "^7.23.3",
        "babel-preset-minify": "^0.5.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "GENERATE_SOURCEMAP=false react-scripts build",
        "lib-build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --extensions '.ts,.tsx,.jsx' --ignore 'src/lib/**/*.test.tsx' --copy-files --no-copy-ignored && cp package.json dist && cp README.md dist",
        "build-publish": "npm run lib-build && cd dist && npm publish"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "resolutions": {
        "babel-loader": "8.1.0"
    }
}